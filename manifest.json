{
  "manifest_version": 2,
  "name": "Not My President",
  "description": "This extension takes us to a better time",
  "version": "1.0",
  "background": {
    "scripts": ["bg.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "webRequest",
    "https://en.wikiquote.org/*",
    "*://whitehouse.gov/*",
    "*://www.whitehouse.gov/*",
    "webRequestBlocking"
  ]
}
